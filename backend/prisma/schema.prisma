generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Company{
  cid Int @default(autoincrement()) @id
  cname String
  password String
}

model allData{
    ID	String @id
    Scope	String
    Level1 String
  	Level2 String
    Level3 String
    Level4	String
    ColumnText	String
    UOM	String
    GHGUnit	String
    GHGConversionFactor Int
}